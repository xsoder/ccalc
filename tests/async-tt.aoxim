import "async.aoxim"

task1 = async(lambda x: {
    x * 2
}, 5)


task2 = async(lambda x, y: {
    x * y * 2
}, 5, 20)


func(z, a) = {
    z * a
}
task3 = async(func, 50, 40)

then(task1, lambda run: {print("Task-1: {run}")})
then(task2, lambda run: {print("Task-2: {run}")})
then(task3, lambda run: {print("Task-3: {run}")})

run_loop()
print("")
