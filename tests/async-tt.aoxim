import "async.aoxim"
task1 = async(lambda x: {
    x * 2
}, 5)


task2 = async(lambda x, y: {
    x * y * 2
}, 5, 20)

func(z, a) = {
    z * a
}

task3 = async(func, 30, 40)

then(task1, lambda result: {print("T-1: {result}")})
then(task2, lambda result: {print("T-2: {result}")})
then(task3, lambda result: {print("T-3: {result}")})

run_loop()
print("")
