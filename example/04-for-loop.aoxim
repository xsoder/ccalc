x = [ "buffer", "test", "nun", "mom" ]

is_palindrome(buffer) = {
    rev = ""
    i = 0
    for j: (buffer) {
        rev = rev + buffer[len(buffer) - 1 - i]
        i = i + 1
    }
    if rev == buffer: { return bool(1)  }
    else: { return bool(0) }
}

for q: (x) {
    e = is_palindrome(q)
    print("Is string '{q}' palindrome => {e}")
}
